  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "port": 9222,
      "name": "Chromium debug",
      "webRoot": "${workspaceFolder}/src",
      "runtimeArgs": [
        "--incognito",
        "--new-window",
        "--remote-debugging-port=9222",
        "--user-data-dir=/home/brainless/Workspaces/chromium",
        "http://dwata.devel"
      ],
      "runtimeExecutable": "/usr/bin/chromium"
    }
  ]
}
